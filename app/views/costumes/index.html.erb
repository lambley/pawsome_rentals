<div class="page-top">
  <h1>Costumes</h1>
    <!--<%= render 'shared/map' %>-->
    <div class="map-container"
      data-controller="map"
      data-map-markers-value="<%= @markers.to_json %>"
      data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>"></div>

  <div class="costumes-card-container">
    <% @costumes.each do |costume| %>
      <%= render 'shared/card', costume: costume %>
    <% end %>
  </div>
</div>
